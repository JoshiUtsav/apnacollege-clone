{
    "version": 2,
    "builds": [
      {
        "src": "./src/index.ts",
        "use": "@vercel/node",
        "config": { "outDir": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ]
  }
  